{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": false,
        "declaration": false,
        "outDir": "dist",
        "jsx": "react",
        "alwaysStrict": true,
        "strict": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
}